<% provide(:title, @title) %>
<% provide(:page_wrapper, "main_wrapper") %>
<div class="row">
  <%= render 'shared/aside_user_info', object: @user %>

  <div class="col-md-8">
    <h3><%= @title %></h3>
    <% if @users.any? %>
      <ul class="users follow">
        <%= render @users %>
      </ul>
      <%= will_paginate %>
    <% end %>
  </div>
</div>